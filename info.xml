<?xml version="1.0"?>
<extension key="supportcase" type="module">
  <file>supportcase</file>
  <name>Support Case</name>
  <description>Manage Support Cases in CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/supportcase</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/supportcase</url>
    <url desc="Support">https://github.com/greenpeace-cee/supportcase/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-10-09</releaseDate>
  <version>1.1.0-beta.3</version>
  <develStage>beta</develStage>
  <requires>
    <ext>mailutils</ext>
  </requires>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Supportcase</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <upgrader>CRM_Supportcase_Upgrader</upgrader>
</extension>
